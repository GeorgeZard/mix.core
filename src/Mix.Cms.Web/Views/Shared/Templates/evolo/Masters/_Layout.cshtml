@{ 
    string assetFolder = Mix.Cms.Lib.MixCmsHelper.GetAssetFolder(ViewBag.culture);
}
<!DOCTYPE html>
<html lang="@ViewBag.culture" amp>

<head>
       <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <!-- SEO Meta Tags -->
    <meta name="description" content="Create a stylish landing page for your business startup and get leads for the offered services with this HTML landing page template.">
    <meta name="author" content="Inovatik">

    <!-- OG Meta Tags to improve the way the post looks when you share the page on LinkedIn, Facebook, Google+ -->
	<meta property="og:site_name" content="" /> <!-- website name -->
	<meta property="og:site" content="" /> <!-- website link -->
	<meta property="og:title" content=""/> <!-- title shown in the actual shared post -->
	<meta property="og:description" content="" /> <!-- description shown in the actual shared post -->
	<meta property="og:image" content="" /> <!-- image link, make sure it's jpg -->
	<meta property="og:url" content="" /> <!-- where do you want your post to link to -->
	<meta property="og:type" content="article" />

    <!-- Website Title -->
    <title>Τσακίρης Γιάννης</title>
    
    <!-- Styles -->
    <link href="https://fonts.googleapis.com/css?family=Raleway:400,400i,600,700,700i&amp;subset=latin-ext" rel="stylesheet">
    <link href="/content/templates/evolo/assets/web/css/bootstrap.css" rel="stylesheet">
    <link href="/content/templates/evolo/assets/web/css/fontawesome-all.css" rel="stylesheet">
    <link href="/content/templates/evolo/assets/web/css/swiper.css" rel="stylesheet">
	<link href="/content/templates/evolo/assets/web/css/magnific-popup.css" rel="stylesheet">
	<link href="/content/templates/evolo/assets/web/css/styles.css" rel="stylesheet">
	
	<!-- Favicon  -->
    <link rel="icon" href="@(Mix.Cms.Lib.Services.MixService.GetConfig<string>("Favicon", ViewBag.culture))">
    
    <link rel="stylesheet" href="~/css/shared.min.css" />
    <link rel="stylesheet" href="~/css/app-vendor.min.css" />
    <base href="/@ViewBag.culture">
    
    @RenderSection("Seo", false)     
    <!--[STYLES]-->
    @RenderSection("Styles", false)    
</head>

<body class="@ViewData["PageClass"]" data-spy="scroll" data-target=".fixed-top" ng-app="MixClient" ng-controller="AppClientController" ng-init="init('@ViewBag.culture')">
    <!-- Preloader -->
    <div class="spinner-wrapper">
        <div class="spinner">
            <div class="bounce1"></div>
            <div class="bounce2"></div>
            <div class="bounce3"></div>
        </div>
    </div>
    <!-- end of preloader -->

  

    @RenderBody()
    <hr>
    @await Html.PartialAsync("../Layouts/_Footer.cshtml")

    <!--   Core JS Files   -->
    <script src="~/js/framework.min.js"></script>
    <script src="~/js/app-client-required.min.js"></script>
    <script src="~/js/app-client.min.js"></script>
    <script src="~/js/app-shared.min.js"></script>

    <!--[SCRIPTS]-->
    <script src="/content/templates/evolo/assets/web/js/jquery.min.js"></script> <!-- jQuery for Bootstrap's JavaScript plugins -->
    <script src="/content/templates/evolo/assets/web/js/popper.min.js"></script> <!-- Popper tooltip library for Bootstrap -->
